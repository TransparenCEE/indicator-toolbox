{
  "ignores": [
    "*.css"
  ],
  "mappings": [
    {
      "src": "scss",
      "dest": "css"
    }
  ],
  "options": {
    "autoprefix": true,
    "autoprefixConfig": "> 1%, last 2 versions, Firefox ESR, Opera 12.1",
    "debugInfo": false,
    "lineComments": false,
    "outputStyle": "compressed",
    "sourceMap": false,
    "unixNewlines": true
  }
}